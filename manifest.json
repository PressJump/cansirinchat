{
    "manifest_version": 2,
    "name": "can-everything",
    "version": "0.0.1",

    "permissions": ["menus", "tabs", "<all_urls>", "theme"],

    "background": {
        "scripts": ["background.js"]
    },

    //web accessible can.gif
    "web_accessible_resources": ["can.gif"],

    "content_scripts": [
        {
            "all_frames": true,
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
